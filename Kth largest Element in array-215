class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
     priority_queue<int>pq(nums.begin(),nums.end());
    // Step 2: k-1 elements pop kar do (sabse bade nikalte rahenge)
        for(int i = 0; i < k - 1; i++) {
            pq.pop();
        }
        return pq.top();
    }
};
