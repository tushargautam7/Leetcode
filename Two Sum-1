class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int, int> mp; 
        for (int i = 0; i < nums.size(); i++) {
            int bacha = target - nums[i]; 
            if (mp.find(bacha) != mp.end()) {
                return {mp[bacha], i}; 
            }
            mp[nums[i]] = i;
        }
        return {}; 
    }
};
